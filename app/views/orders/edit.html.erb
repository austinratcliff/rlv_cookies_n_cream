<%= form_with model: @order, method: :put do |form| %>
  <%= form.label :count, 'Count:' %><br>
  <%= form.select :count, options_from_collection_for_select(@counts, :first, :last, @order.count) %><br>
  <%= form.label :description, 'Description:' %><br>
  <%= form.text_area :description %><br>
  <%= form.label :is_delivery, 'Delivery?' %><br>
  <%= form.check_box :is_delivery, {checked: @order.is_delivery}, true, false %><br>
  <%= form.submit 'Next' %>
<% end %>
